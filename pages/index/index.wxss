@keyframes slideInUp {
    from {
        transform: translate3d(0,20%,0);
        opacity: 0;
    }

    to {
        transform: translate3d(0,0,0);
        opacity: 1;
    }
}

.slideInUp {
    animation: slideInUp .3s ease 1 forwards;
}

@-webkit-keyframes slideOutDown {
    from {
        opacity: 1;
        transform: translate3d(0,0,0);
    }

    to {
        opacity: 0;
        transform: translate3d(0,20%,0);
    }
}

@keyframes slideOutDown {
    from {
        opacity: 1;
        transform: translate3d(0,0,0);
    }

    to {
        opacity: 0;
        transform: translate3d(0,20%,0);
    }
}

.slideOutDown {
    animation: slideOutDown .3s ease 1 forwards;
}